{
  "suggest.noselect": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "javascript",
    "json",
    "jsonc",
    "yaml",
    "go"
  ],
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".git"
      ],
      "trace.server": "off",
      "filetypes": [
        "go"
      ]
    }
  }
}
